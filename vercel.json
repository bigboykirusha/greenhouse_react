{
   "version": 2,
   "builds": [
      {
         "src": "src/**/*",
         "use": "@vercel/static-build"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "headers": [
            {
               "source": "/(.*).(jpg|jpeg|png|gif|ico|css|js|webp)",
               "headers": [
                  {
                     "key": "Cache-Control",
                     "value": "public, max-age=2592000, s-maxage=2592000, immutable"
                  }
               ]
            },
            {
               "source": "/(.*).svg",
               "headers": [
                  {
                     "key": "Cache-Control",
                     "value": "public, max-age=31536000, s-maxage=31536000, immutable"
                  }
               ]
            }
         ]
      }
   ]
}